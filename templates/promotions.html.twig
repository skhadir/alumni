{% extends 'base.html.twig' %}

{% block body %}
        <h2>Promotions</h2>
    <ul>
            {% for promotion in promotions %}
                        <li>
                            <a href="{{ path('promotion.index', {"id": promotion.id}) }}">
                                {{ promotion.degree.name }} - {{ promotion.year.title }}
                            </a>
                        </li>
            {% endfor %}
    </ul>
{% endblock %}