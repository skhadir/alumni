{% extends 'admin/index.html.twig' %}
{% block body %}
    <h2>Promotions</h2>

    {% for promotion in promotions %}
            <li>
                <a href="{{ path('') }}" {{ promotion.degree.name }} - {{ promotion.year.title }}
            </li>
            <li>{{ promotion.startdate }}</li>
            <li>{{ promotion.enddate }}</li>
    {% endfor %}

{% endblock %}